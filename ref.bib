@article{cole:2016,
    abstract = {Substrate-transferred crystalline coatings have recently emerged as a groundbreaking new concept in optical interference coatings. Building upon our initial demonstration of this technology, we have now realized significant improvements in the limiting optical performance of these novel single-crystal GaAs/AlxGa1\&\#x2212;xAs multilayers. In the near-infrared (NIR), for coating center wavelengths spanning 1064--1560 nm, we have reduced the excess optical losses (scatter $+$ absorption) to levels as low as 3 parts per million (ppm), enabling the realization of a cavity finesse exceeding 3\&\#x00D7;105 at the telecom-relevant wavelength range near 1550 nm. Moreover, we demonstrate the direct measurement of sub-ppm optical absorption at 1064 nm. Concurrently, we investigate the mid-infrared (MIR) properties of these coatings and observe exceptional performance for first attempts in this important wavelength region. Specifically, we verify excess losses at the hundred ppm level for wavelengths of 3300 and 3700 nm. Taken together, our NIR optical losses are now fully competitive with ion-beam sputtered multilayer coatings, while our first prototype MIR optics have already reached state-of-the-art performance levels for reflectors covering this portion of the fingerprint region for optical gas sensing. Mirrors fabricated with our crystalline coating technique exhibit the lowest mechanical loss, and thus the lowest Brownian noise, the highest thermal conductivity, and, potentially, the widest spectral coverage of any \&\#x201C;supermirror\&\#x201D; technology in a single material platform. Looking ahead, we see a bright future for crystalline coatings in applications requiring the ultimate levels of optical, thermal, and optomechanical performance.},
    author = {Garrett D. Cole and Wei Zhang and Bryce J. Bjork and David Follman and Paula Heu and Christoph Deutsch and Lindsay Sonderhouse and John Robinson and Chris Franz and Alexei Alexandrovski and Mark Notcutt and Oliver H. Heckl and Jun Ye and Markus Aspelmeyer},
    doi = {10.1364/OPTICA.3.000647},
    journal = {Optica},
    keywords = {Optical resonators; Semiconductor materials ; Bragg reflectors; Absorption; Interference coatings ; Deposition and fabrication; Fourier transform infrared spectroscopy; Material properties; Optical coatings; Optical properties; Optical sensing; Thin films},
    month = {Jun},
    number = {6},
    pages = {647--656},
    publisher = {OSA},
    title = {High-performance near- and mid-infrared crystalline coatings},
    url = {http://opg.optica.org/optica/abstract.cfm?URI=optica-3-6-647},
    volume = {3},
    year = {2016},
    Bdsk-Url-1 = {http://opg.optica.org/optica/abstract.cfm?URI=optica-3-6-647},
    Bdsk-Url-2 = {https://doi.org/10.1364/OPTICA.3.000647}
}

